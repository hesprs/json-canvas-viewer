{
	"name": "json-canvas-viewer",
	"version": "3.3.0",
	"description": "An extensible web-based viewer for JSON Canvas, easy to embed into websites.",
	"type": "module",
	"types": "dist/types/index.d.ts",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"exports": {
		".": {
			"types": "./dist/types/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./dev": {
			"types": "./dist/types/dev.d.ts",
			"import": "./dist/dev.js",
			"require": "./dist/dev.cjs"
		},
		"./modules": {
			"types": "./dist/types/modules.d.ts",
			"import": "./dist/modules.js",
			"require": "./dist/modules.cjs"
		},
		"./bridges": {
			"types": "./dist/types/bridges.d.ts",
			"import": "./dist/bridges.js",
			"require": "./dist/bridges.cjs"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hesprs/json-canvas-viewer.git"
	},
	"homepage": "https://github.com/hesprs/json-canvas-viewer#readme",
	"bugs": {
		"url": "https://github.com/hesprs/json-canvas-viewer/issues"
	},
	"keywords": ["obsidian", "json-canvas", "front-end", "html-canvas", "typescript"],
	"author": {
		"name": "HeÌ„sperus",
		"email": "hesprs@outlook.com"
	},
	"sideEffects": false,
	"publishConfig": {
		"provenance": true,
		"access": "public"
	},
	"license": "MIT",
	"packageManager": "pnpm@10.24.0",
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@types/node": "^25.0.6",
		"dompurify": "^3.3.1",
		"marked": "^17.0.1",
		"sass": "^1.97.2",
		"tsc-alias": "^1.8.16",
		"typescript": "^5.9.3",
		"vite": "^7.3.1"
	},
	"files": ["dist"],
	"scripts": {
		"dev": "vite",
		"lint": "biome check --write",
		"build": "vite build && BUILD=chimp vite build && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
		"check": "tsc --noEmit && biome check"
	},
	"dependencies": {
		"@needle-di/core": "^1.1.0",
		"pointeract": "^1.0.1"
	}
}
